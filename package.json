{
  "name": "timezeropw",
  "description": "TimeZeroPW client",
  "author": "ravoit",
  "license": "MIT",
  "version": "1.1.7",
  "main": "./main.js",
  "scripts": {
    "run": "electron .",
    "publish": "electron-builder build -p always"
  },
  "build": {
    "appId": "pw.timezero",
    "productName": "TimeZeroPW",
    "extraFiles": "game/**/*",
    "files": "!lib/",
    "win": {
      "extraResources": "lib/pepflashplayer32.dll",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^6.0.0",
    "electron-builder": "^22.1.0"
  },
  "dependencies": {
    "electron-store": "^5.1.0",
    "electron-updater": "^4.0.0",
    "finalhandler": "latest",
    "serve-static": "latest",
    "swfobject": "^2.2.1"
  }
}
