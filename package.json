{
  "name": "timezeropw",
  "description": "TimeZeroPW client",
  "author": "ravoit",
  "license": "MIT",
  "version": "1.2.1",
  "main": "./main.js",
  "scripts": {
    "run": "electron .",
    "release": "electron-builder build -p always -mwl"
  },
  "build": {
    "appId": "pw.timezero",
    "productName": "TimeZeroPW",
    "extraFiles": "game/**/*",
    "linux": {
      "category": "game",
      "target": ["Appimage","tar.gz"]
    }
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "^22.8.0"
  },
  "dependencies": {
    "electron-store": "latest",
    "electron-updater": "^4.0.0",
    "finalhandler": "latest",
    "serve-static": "latest",
    "swfobject": "latest"
  }
}